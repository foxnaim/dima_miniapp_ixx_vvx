{
  "name": "dima_miniapp_ixx_vvx",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "dev:frontend": "next dev",
    "dev:backend": "cd backend && python3 -m uvicorn app.main:app --reload --port 8000",
    "dev:full": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\"",
    "build": "next build",
    "build:frontend": "next build",
    "build:backend": "echo 'Backend does not need build step'",
    "build:full": "yarn build:frontend && yarn build:backend",
    "start": "next start",
    "start:backend": "cd backend && python3 -m uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "start:full": "concurrently \"yarn start:backend\" \"yarn start\"",
    "lint": "eslint .",
    "docker:build": "docker build -t dima-miniapp .",
    "docker:run": "docker run -p 8000:8000 --env-file .env dima-miniapp",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up -d --build",
    "docker:compose:down": "docker-compose down"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
